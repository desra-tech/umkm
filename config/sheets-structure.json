{
  "spreadsheet_name": "UMKM Financial System Database",
  "sheets": [
    {
      "name": "Users",
      "description": "Menyimpan data pengguna dan level akses",
      "columns": [
        "User ID",
        "Username",
        "Password Hash",
        "Salt",
        "Full Name",
        "Role",
        "Email",
        "Phone",
        "Status",
        "Created Date",
        "Last Login"
      ]
    },
    {
      "name": "Accounts",
      "description": "Chart of Accounts - Daftar akun untuk pembukuan",
      "columns": [
        "Account Code",
        "Account Name",
        "Account Type",
        "Category",
        "Parent Account",
        "Description",
        "Status",
        "Created Date"
      ]
    },
    {
      "name": "Customers",
      "description": "Daftar pelanggan",
      "columns": [
        "Customer ID",
        "Customer Name",
        "Contact Person",
        "Phone",
        "Email",
        "Address",
        "City",
        "Tax ID",
        "Credit Limit",
        "Status",
        "Created Date"
      ]
    },
    {
      "name": "Suppliers",
      "description": "Daftar pemasok",
      "columns": [
        "Supplier ID",
        "Supplier Name",
        "Contact Person",
        "Phone",
        "Email",
        "Address",
        "City",
        "Tax ID",
        "Payment Terms",
        "Status",
        "Created Date"
      ]
    },
    {
      "name": "Products",
      "description": "Daftar barang/jasa",
      "columns": [
        "Product ID",
        "Product Code",
        "Product Name",
        "Category",
        "Unit",
        "Purchase Price",
        "Selling Price",
        "Stock",
        "Min Stock",
        "Description",
        "Status",
        "Created Date"
      ]
    },
    {
      "name": "Transactions",
      "description": "Transaksi keuangan utama",
      "columns": [
        "Transaction ID",
        "Date",
        "Transaction Type",
        "Reference No",
        "Account Code",
        "Debit",
        "Credit",
        "Description",
        "Partner ID",
        "Partner Type",
        "Created By",
        "Created Date",
        "Status"
      ]
    },
    {
      "name": "Sales",
      "description": "Transaksi penjualan (POS)",
      "columns": [
        "Sale ID",
        "Sale Date",
        "Invoice No",
        "Customer ID",
        "Product ID",
        "Quantity",
        "Unit Price",
        "Discount",
        "Tax",
        "Total",
        "Payment Method",
        "Created By",
        "Created Date",
        "Status"
      ]
    },
    {
      "name": "Purchases",
      "description": "Transaksi pembelian",
      "columns": [
        "Purchase ID",
        "Purchase Date",
        "PO No",
        "Supplier ID",
        "Product ID",
        "Quantity",
        "Unit Price",
        "Discount",
        "Tax",
        "Total",
        "Payment Status",
        "Created By",
        "Created Date",
        "Status"
      ]
    },
    {
      "name": "Receivables",
      "description": "Piutang usaha",
      "columns": [
        "Receivable ID",
        "Invoice No",
        "Invoice Date",
        "Due Date",
        "Customer ID",
        "Amount",
        "Paid Amount",
        "Balance",
        "Status",
        "Created By",
        "Created Date"
      ]
    },
    {
      "name": "Payables",
      "description": "Utang usaha",
      "columns": [
        "Payable ID",
        "Invoice No",
        "Invoice Date",
        "Due Date",
        "Supplier ID",
        "Amount",
        "Paid Amount",
        "Balance",
        "Status",
        "Created By",
        "Created Date"
      ]
    },
    {
      "name": "Inventory",
      "description": "Pergerakan stok barang",
      "columns": [
        "Movement ID",
        "Date",
        "Product ID",
        "Transaction Type",
        "Reference No",
        "Quantity In",
        "Quantity Out",
        "Balance",
        "Notes",
        "Created By",
        "Created Date"
      ]
    },
    {
      "name": "Settings",
      "description": "Pengaturan sistem",
      "columns": [
        "Setting Key",
        "Setting Value",
        "Description",
        "Updated Date"
      ]
    }
  ],
